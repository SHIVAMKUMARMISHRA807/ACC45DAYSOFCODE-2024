#include <iostream>
using namespace std;

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    int t;
    int x,n,s;
    cin>>t;
    int a,b;
    
    while(t--){
        cin>>x>>n>>s;
        while(s--){
            cin>>a>>b;
            if(a==n){n=b;}
            else if(b==n){n=a;}
        }
        
    cout<<n<<"\n";
    }
    
	return 0;
}
