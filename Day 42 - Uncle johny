#include<bits/stdc++.h>
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n,k;
	    cin>>n;
	    int a[n];
	    for(int i=0;i<n;i++){
	      cin>>a[i];
	    }
	    cin>>k;
	    int p=a[k-1];
	    sort(a,a+n);
	    for(int i=0;i<n;i++){
	        if(a[i]==p){
	            cout<<i+1<<endl;
	        }
	    }
	    
	    
	}
	return 0;
}
